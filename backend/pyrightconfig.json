{
  "venvPath": ".",
  "venv": ".venv",
  "pythonVersion": "3.13",
  "pythonPlatform": "Windows",
  "executionEnvironments": [
    {
      "root": ".",
      "extraPaths": [
        "./.venv/Lib/site-packages",
        "./.venv/lib/python3.13/site-packages"
      ]
    }
  ],
  "include": [
    "."
  ],
  "exclude": [
    "**/node_modules",
    "**/__pycache__",
    ".git"
  ],
  "ignore": [
    "*.pyc",
    "__pycache__"
  ],
  "reportMissingImports": true,
  "reportMissingTypeStubs": false
}